# Authentication mode
# 'true' = use custom CMIPS login page (backend proxies to Keycloak)
# 'false' = use Keycloak redirect flow (Keycloak's own login page)
REACT_APP_DISABLE_AUTH=true

# Keycloak server URL
REACT_APP_KEYCLOAK_URL=http://localhost:8080

# Keycloak realm name
REACT_APP_KEYCLOAK_REALM=cmips

# Keycloak client ID for this application
REACT_APP_KEYCLOAK_CLIENT_ID=cmips-frontend

# Backend API base URL (no trailing /api)
REACT_APP_API_BASE_URL=http://localhost:8081

# Optional: Application base URL
REACT_APP_BASE_URL=http://localhost:3000
